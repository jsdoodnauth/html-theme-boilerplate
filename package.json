{
  "name": "material-theme-ui",
  "version": "0.5.0",
  "author": "Joshua Doodnauth",
  "description": "A minimalist front-end design toolkit built with Sass for developing responsive, browser-consistent web apps.",
  "main": "index.js",
  "dependencies": {
    "node-sass": "^4.12.0"
  },
  "scripts": {
    "sass": "node-sass ./src/scss/ -o ./dist/css/ -w -r --source-map-embed --source-map-contents --output-style expanded | node-sass ./src/scss/materialize.scss ./dist/css/main.min.css -w --output-style compressed",
    "sass:docs": "node-sass -w -r ./src/scss/ -o ./docs/css/"
  },
  "keywords": [
    "sass",
    "setup",
    "boilerplate",
    "framework",
    "ui",
    "css framework",
    "front end",
    "design"
  ],
  "license": "MIT",
  "private": false
}
